{
  "name": "sampleman",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node ./bin/www"
  },
  "pkg": {
    "scripts": [
      "scripts/**/*.js",
      "node_modules/**/*.js"
    ],
    "assets": [
      "views/**/*",
      "public/**/*"
    ],
    "outputPath": "releases/"
  },
  "bin": "./bin/www",
  "dependencies": {
    "cookie-parser": ">=1.4.3",
    "deepmerge": ">=4.2.2",
    "express": ">=4.15.5",
    "fs-extra": ">=10.0.0",
    "marked": ">=0.3.6",
    "morgan": ">=1.9.0",
    "multer": ">=1.3.0",
    "open": ">=8.2.1",
    "pug": ">=2.0.0",
    "serve-favicon": ">=2.4.5",
    "www": ">=0.0.0",
    "yaml": ">=1.10.2"
  }
}
